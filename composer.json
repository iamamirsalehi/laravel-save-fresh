{
    "name": "iamamirsalehi/laravel-save-fresh",
    "description": "Don't worry about fresh your database because all data will back to your tables",
    "license": "mit",
    "authors": [
        {
            "name": "Amir Salehi",
            "email": "iamamirsalehi@gmail.com",
            "homepage": "https://github.com/iamamirsalehi/"
        }
    ],
    "homepage": "https://github.com/iamamirsalehi/laravel-save-fresh",
    "keywords": ["Laravel", "LaravelSaveFresh"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6",
        "spatie/laravel-backup": "^6.13",
        "nelexa/zip": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Iamamirsalehi\\LaravelSaveFresh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iamamirsalehi\\LaravelSaveFresh\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iamamirsalehi\\LaravelSaveFresh\\LaravelSaveFreshServiceProvider"
            ],
            "aliases": {
                "LaravelSaveFresh": "Iamamirsalehi\\LaravelSaveFresh\\Facades\\LaravelSaveFresh"
            }
        }
    }
}
